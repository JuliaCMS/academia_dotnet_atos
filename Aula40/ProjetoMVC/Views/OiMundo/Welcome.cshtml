@{
    ViewData["Title"] = "Apresentando a mensagem algumas vezes";
}

<h1>Olá, sejam bem vindes...</h1>
<p>@ViewData["Message"]</p>
<p>@ViewData["NumTimes"]</p>

<ul>
    @for (int i = 0; i < (int)@ViewData["NumTimes"]; i++)
    {
        <li>@i - @ViewData["Message"]</li>
    }
</ul>
